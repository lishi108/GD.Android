<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <!--<include layout="@layout/app_bar_base" />-->
    <RelativeLayout
        android:id="@+id/aboutAppraiseLayout"
        android:layout_marginTop="@dimen/dp10"
        style="@style/Setting_Layout">

        <ImageView
            android:id="@+id/aboutIcon1"
            style="@style/Setting_IconLeft"
            android:src="@mipmap/icon_def_qq" />

        <TextView
            style="@style/Setting_Text"
            android:layout_toRightOf="@id/aboutIcon1"
            android:text="@string/about_appraise" />

        <ImageView
            style="@style/Setting_IconRight"/>


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/aboutProtocolLayout"
        android:layout_marginTop="@dimen/dp10"
        style="@style/Setting_Layout">

        <ImageView
            android:id="@+id/aboutIcon2"
            style="@style/Setting_IconLeft"
            android:src="@mipmap/icon_def_qq" />

        <TextView
            style="@style/Setting_Text"
            android:layout_toRightOf="@id/aboutIcon2"
            android:text="@string/about_protocol" />

        <ImageView
            style="@style/Setting_IconRight"/>


    </RelativeLayout>

</LinearLayout> 
